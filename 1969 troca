quant_de_testes = int(input())
for c in range(quant_de_testes):
  entrada = input()
  digito1 = int(entrada[0])
  letra1 = entrada[1]
  digito2 = int(entrada[2])
  if digito1 == digito2:
    print(digito1 * digito2)
  elif letra1.isupper():
    print(digito2 - digito1)
  elif letra1.islower():
    print(digito1 + digito2)
